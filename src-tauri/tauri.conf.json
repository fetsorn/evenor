{
  "productName": "evenor",
  "version": "2.5.0",
  "identifier": "com.evenor",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420/",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "evenor",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "copyright": "GPLv3",
    "category": "Utilities",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "entitlements": "./entitlements.plist"
    },
    "iOS": {
      "developmentTeam": "PU6CTXHGJ5"
    }
  },
  "plugins": {}
}
