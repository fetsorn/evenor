  "description": "Tell the story of your data.",
  "type": "module",
  "main": "./.webpack/main",
  "scripts": {
    "build": "webpack --config webpack.web.config.mjs --mode='production'",
    "dev": "webpack serve --config webpack.web.config.mjs --port 1420",
    "build:tauri": "webpack --config webpack.web.config.mjs --mode='production' --env buildMode=tauri",
    "dev:tauri": "webpack serve --config webpack.web.config.mjs --port 1420 --env buildMode=tauri",
    "tauri": "tauri",
    "build:server": "webpack --env buildMode=server --mode='development' --config webpack.web.config.mjs; ln -sTf $(realpath release/renderer) src-server/build; cd src-server; yarn",
    "dev:server": "node ./src-server/index.js",
    "lint": "eslint ."
  },
